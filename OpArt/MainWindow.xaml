<Window x:Class="OpArt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpArt"
        mc:Ignorable="d"
        Title="OpArt Generator" Height="450" Width="800">
    <Grid x:Name="LayoutRoot" Loaded="LayoutRootLoaded" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="HirstGrid" Grid.Column="0" />
        <Grid x:Name="RileyGrid" Grid.Column="1" />
    </Grid>
</Window>
